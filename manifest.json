{
	"manifest_version": 2,
	"name": "Wiki Desktop Redirector",
	"description": "Redirects Wikipedia's (and other Wikimedia projects') mobile pages to their desktop versions.",
	"version": "0.0.0",
	"icons": {
		"16": "icons/icon-16.png",
		"32": "icons/icon-32.png",
		"48": "icons/icon-48.png",
		"64": "icons/icon-64.png",
		"128": "icons/icon-128.png"
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"*://*.m.mediawiki.org/*",
		"*://*.m.wikibooks.org/*",
		"*://*.m.wikidata.org/*",
		"*://*.m.wikimedia.org/*",
		"*://*.m.wikimediafoundation.org/*",
		"*://*.m.wikinews.org/*",
		"*://*.m.wikipedia.org/*",
		"*://*.m.wikiquote.org/*",
		"*://*.m.wikisource.org/*",
		"*://*.m.wikiversity.org/*",
		"*://*.m.wikivoyage.org/*",
		"*://*.m.wiktionary.org/*"
	],
	"background": {
		"scripts": [
			"background.js"
		]
	}
}
